{
  "name": "craftcms/cloud",
  "type": "yii2-extension",
  "require": {
    "craftcms/cms": "^4.2",
    "yiisoft/yii2-redis": "^2.0",
    "yiisoft/yii2-queue": "^2.3",
    "craftcms/flysystem": "dev-main",
    "league/uri": "^6.7",
    "bref/bref": "^2.0",
    "bref/extra-php-extensions": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "craft\\cloud\\": "src/"
    }
